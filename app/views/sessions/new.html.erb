<h2>Sign in</h2>
<%= form_for(:session, url: login_path) do |f| %>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <%= f.submit "Sign in", class: 'btn btn-default'%>
<% end %>
<br>
<p>Don't have an account? <%= link_to("Create an account here.", new_user_path) %></p>